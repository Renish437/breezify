{
    "name": "codesren/breezify",
    "description": "This is a combination of Laravel Breeze UI and Fortify as a backend authentication system",
    "type": "laravel-package",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "CodesRen\\Breezify\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Renish Siwakoti"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "laravel/fortify": "^1.0",
        "laravel/breeze": "^1.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodesRen\\Breezify\\Providers\\BreezifyServiceProvider"
            ]
        }
    }
}